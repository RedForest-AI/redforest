{
  "expo": {
    "name": "RedForest",
    "slug": "redforest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/favicon/favicon-32x32.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/favicon/favicon-32x32.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "redforest",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.redforest.ai"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/favicon/favicon-32x32.png",
        "backgroundColor": "#000000"
      },
      "package": "com.redforest.ai"
    },
    "web": {
      "favicon": "./assets/favicon/favicon-32x32.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ],
    "extra": {
      "router": {
        "origin": false
      },
      "eas": {
        "projectId": "a63f303a-fe26-4834-abc4-47ed6db0afd4"
      }
    },
    "owner": "redforest"
  }
}
