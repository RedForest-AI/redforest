{
  "expo": {
    "name": "RedForest",
    "slug": "RedForest",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/icon.png",
    "userInterfaceStyle": "light",
    "splash": {
      "image": "./assets/splash.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "scheme": "com.redforest.app",
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/adaptive-icon.png",
        "backgroundColor": "#000000",
        "intentFilters": [
          {
            "action": "VIEW",
            "data": {
              "scheme": "https",
              "host": "*.redforest.app",
              "pathPrefix": "/",
              "pathPattern": ".*"
            },
            "category": [
              "BROWSABLE",
              "DEFAULT"
            ]
          }
        ]
      }
    },
    "web": {
      "favicon": "./assets/favicon.png",
      "bundler": "metro"
    },
    "plugins": [
      "expo-router"
    ]
  }
}
